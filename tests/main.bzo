add :: [n':Num] => [a:n', b:n'] -> [n'] {
	a + b
}


stddev :: [n':Num] => [xs:[]n'] -> [n', n'] {
	|: [#req: xs.len > 0]
	avg := [fold: xs, 0, add] / [len:xs]
	dev := [map : xs, [x]{
				(x-avg)^2
			}
			] \ [fold: _, 0, add] / ([len:xs]-1)
	[return: avg, dev]
}


