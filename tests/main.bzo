#: code
stddev :: [xs : []F64] -> [F64, F64] {
	avg := [fold: xs, 0, (+)] / [len:xs]
	dev := [map : xs, [x]{
				(x-avg)^2
			}] [fold: _, 0, (+)] / ([len:xs]-1)
	avg, dev
}




